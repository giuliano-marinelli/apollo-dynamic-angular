{
  "name": "apollo-dynamic-angular",
  "version": "1.0.4",
  "description": "Apollo Angular but Dynamic! It adds the ability to create dynamic selection sets on queries, mutations and subscriptions, based on decorated schema.",
  "author": "Giuliano Marinelli",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/fesm2020/ngApolloDynamic.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && ng-packagr -p ng-package.json",
    "watch": "rimraf dist && ng-packagr -p ng-package.json --watch",
    "pack": "npm run build && cd dist && npm pack --pack-destination=..",
    "publish": "npm run build && cd dist && npm publish"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.6",
    "@angular-devkit/core": "^20.0.6",
    "@angular-devkit/schematics": "^20.0.6",
    "@angular/compiler": "^20.0.6",
    "@angular/compiler-cli": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@apollo/client": "^3.13.8",
    "@types/node": "^20.11.19",
    "apollo-angular": "^11.0.0",
    "apollo-dynamic": "^1.0.4",
    "cp": "^0.2.0",
    "graphql": "^16.11.0",
    "ng-packagr": "^20.1.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.2",
    "ts-morph": "^20.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@angular/core": ">=20.0.0",
    "@apollo/client": ">=3.13.8",
    "apollo-angular": ">=11.0.0",
    "apollo-dynamic": ">=1.0.4",
    "graphql": ">=16.8.1",
    "rxjs": ">=6.0.0"
  },
  "homepage": "https://github.com/giuliano-marinelli/apollo-dynamic-angular#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/giuliano-marinelli/apollo-dynamic-angular"
  }
}
